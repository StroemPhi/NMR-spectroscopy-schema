      },
      "NmrSolution": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "buffer": {
               "$ref": "#/$defs/NmrBuffer"
            },
            "sample": {
               "$ref": "#/$defs/NmrSample"
            },
            "solvent": {
               "$ref": "#/$defs/NmrSolvent"
            }
         },
         "required": [
            "solvent",
            "sample"
         ],
         "title": "NmrSolution",
         "type": "object"
      },
      "NmrSolvent": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "formula": {
               "type": "string"
            },
            "id": {
               "type": "string"
            },
            "inchi": {
               "type": "string"
            },
            "inchikey": {
               "type": "string"
            },
            "name": {
               "description": "a generic property to provide a string that is used to name a something",
               "type": "string"
            },
            "smiles": {
               "type": "string"
            }
         },
         "required": [
            "id"
         ],
         "title": "NmrSolvent",
         "type": "object"
      },
      "NmrSpecRecord": {
         "additionalProperties": false,
         "description": "This is the tree root node of the schema. It is a data item that serves as a container for all relevant information about one NMR spectroscopy assay record. The properties of this class represent the required metadata of an NMR assay, such as:  * what kind of assay was performed (e.g. 2D 13C COSY) and what devices where used for that (e.g. spectrometer, magnet, solvent, buffer, ...) * what are the detail infos of the assayed sample (name, formula, structure, concentration, preperation process, ...)",
         "properties": {
            "date_retrieved": {
               "format": "date-time",
               "type": "string"
            },
            "id": {
               "type": "string"
            },
            "output_of_nmr_assay": {
               "$ref": "#/$defs/PulsedNmrAssay"
            },
            "source": {
               "description": "a property to be used to provide the source of the data as String",
               "type": "string"
            },
            "source_uri": {
               "description": "a property to be used to provide the source of the data as URL or CURIE",
               "type": "string"
            }
         },
         "required": [
            "id",
            "output_of_nmr_assay"
         ],
         "title": "NmrSpecRecord",
         "type": "object"
      },
      "NmrSpectrometer": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "id": {
               "type": "string"
            },
            "manufactured_by": {
               "$ref": "#/$defs/Manufacturer",
               "description": "a property to be used on any class that represents some form of atrefact, in order to refer to the artefacts maker."
            },
            "name": {
               "description": "a generic property to provide a string that is used to name a something",
               "type": "string"
            },
            "type": {
               "type": "string"
            }
         },
         "required": [
            "id"
         ],
         "title": "NmrSpectrometer",
         "type": "object"
      },
      "PulseProgram": {
         "description": "",
         "enum": [
            "NMR",
            "COSY",
            "COSY-DQF",
            "COSY-DOSY",
            "DOSY",
            "SECSY",
            "RELAY",
            "TOCSY",
            "ROESY",
            "NOESY",
            "Double-Quantum",
            "J-Resolved",
            "DEPT & INEPT",
            "HECTOR",
            "COLOC",
            "Heteronuclear J-resolved",
            "HOESY",
            "INADEQUATE",
            "Inverse 1H-NMR",
            "HMQC",
            "DEPT-HMQC",
            "Multiplicity-edited HSQC",
            "CT-HSQC",
            "CT-HMBC",
            "Inverse-INEPT",
            "2D HSQC-\u03b1,\u03b2",
            "2D IPAP-HSQC",
            "2D J-modulated CT-HSQC",
            "TROSY",
            "CRINEPT",
            "HMQC-COSY",
            "H2BC",
            "HQMC-TOSCY",
            "HMQC-ROESY",
            "HMQC-NOESY",
            "HSQC-TOSCY",
            "HSQC-ROESY",
            "HSQC-NOESY",
            "HMBC",
            "Phase-sensitive HMBC",
            "J-HMBC",
            "Long-range HSQC (HSQMBC)",
            "EXSIDE",
            "HETLOC",
            "HSQC-HECADE",
            "ADEQUATE",
            "1,1-ADEQUATE",
            "1,n-ADEQUATE",
            "n,1-ADEQUATE",
            "n,n-ADEQUATE",
            "STE",
            "STEBP",
            "STD-TOSCY",
            "STD-NOESY",
            "STD-HSQC",
            "CLEANEX",
            "CLEANEX-HSQC",
            "CLEANEX-TROSY",
            "DSTE",
            "DSTEBP",
            "DOSY-TOCSY",
            "DOSY-NOESY",
            "DOSY-HMQC"
         ],
         "title": "PulseProgram",
         "type": "string"
      },
      "PulsedNmrAssay": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "acuisition_nuclei": {
               "description": "the nuclei to be assayed as specified by the protocol",
               "items": {
                  "type": "string"
               },
               "type": "array"
            },
            "assay_date": {
               "description": "the date the assay was carried out by someone",
               "format": "date-time",
               "type": "string"
            },
            "assayed_solution": {
               "$ref": "#/$defs/NmrSolution",
               "description": "the property refering to solution in which the assayed sample is disolved in"
            },
            "has_dimension": {
               "$ref": "#/$defs/Dimension"
            },
            "id": {
               "type": "string"
            },
            "name": {
               "description": "a generic property to provide a string that is used to name a something",
               "type": "string"
            },
            "observed_frequencies": {
               "items": {
                  "type": "number"
               },
               "type": "array"
            },
            "pulse_program": {
               "$ref": "#/$defs/PulseProgram",
               "description": "The pulse program of a PulsedNmrAssay is a required property and must be conform to one of the values defined in the PulseProgram enum class. The possible values there are defined according to common pulse programs, e.g. NMR for basic 1H-NMR, or COSY and COSY-DOSY, etc."
            },
            "spectrometer": {
               "$ref": "#/$defs/NmrSpectrometer"
            }
         },
         "required": [
            "id",
            "assayed_solution",
            "spectrometer",
            "has_dimension",
            "pulse_program",
            "acuisition_nuclei"
         ],
         "title": "PulsedNmrAssay",
         "type": "object"
      }
   },
